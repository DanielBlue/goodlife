<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.zoesap.goodlife.activity.GoldGiveHistoryActivity">
    <include layout="@layout/include_top_toolbar" />
    <RelativeLayout
        android:id="@+id/rl_empty"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:drawableTop="@drawable/e4_expenditure_empty"
            android:text="@string/no_income_history"
            android:drawablePadding="@dimen/base_margin"
            android:textSize="@dimen/margin_big"
            android:textColor="@color/black"/>
    </RelativeLayout>
</FrameLayout>
